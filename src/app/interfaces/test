
export interface Board {
  id?: number;
  name: string;
  columns: Column[];
}


export interface Column {
  id?: number;
  name: string;
  boardId: number;
  tasks ?: Task[];
}


export interface Task {
  id?: number;
  title: string;
  content: string;
  columnId: number;
}


export interface User {
  id?: number;
  username: string;
  mail: string;
}
